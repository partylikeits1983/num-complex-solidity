{
  "singleQuote": false,
  "semi": true,
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": true,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4,
        "useTabs": true,
        "plugins": ["prettier-plugin-solidity"]
      }
    },
    {
      "files": "*.ts",
      "options": {
        "tabWidth": 2,
        "useTabs": true
      }
    }
  ]
}
